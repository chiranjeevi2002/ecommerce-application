package com.ecommerce.common.security;

import java.util.Set;

import static com.ecommerce.common.security.Role.*;

public final class UserRoles {

    private UserRoles() {}

    
    public static final String ADMIN = Role.ADMIN.authority();
    public static final String SUPERADMIN = Role.SUPERADMIN.authority();
    public static final String DEFAULT = Role.CUSTOMER.authority();
}
