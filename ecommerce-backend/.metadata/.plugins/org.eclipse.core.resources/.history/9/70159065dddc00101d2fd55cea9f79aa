public final class RequestContextMapper {

    private RequestContextMapper() {}

    public static RequestContext fromHeaders(
            String userId,
            String storeId,
            String username,
            String roles,
            String authorization
    ) {
        return new RequestContext(
                parseLong(userId),
                parseLong(storeId),
                username,
                parseRoles(roles),
                authorization
        );
    }
